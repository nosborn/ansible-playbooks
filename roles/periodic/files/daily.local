#!/bin/ksh

PATH="/usr/local/sbin:/usr/local/bin:${PATH}"

case "$(uname -r)" in
*-beta | *-current)
  next_part "Available patches:"
  syspatch -c
  ;;
esac

if [ -e /usr/local/libexec/update-dns-blacklist ]; then
  next_part "Update DNS blacklist:"
  nice /usr/local/libexec/update-dns-blacklist
fi
